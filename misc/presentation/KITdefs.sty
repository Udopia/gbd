    \ifx##1\relax \relax
     \else
      \expandafter\KIT@search{h}{##1}
      \ifKIT@match\gdef\KIT@wrap@opt{l}\fi
     \fi
    \begin{wrapfigure}{\KIT@wrap@opt}{\columnwidth}
   }{%
    \end{wrapfigure}
    \let\WF@collision=\relax
    \begin{wrapfigure}[0]{\KIT@wrap@opt}{\columnwidth}
    \end{wrapfigure}
    \rule{0pt}{0pt}%
    \let\WF@collision=\KIT@WF@warning
    \vspace{\KITintextsep}
    \par
   }
  \renewenvironment{table}[1][\relax]{%
    \gdef\KIT@wrap@opt{L}
